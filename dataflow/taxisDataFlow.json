{
	"name": "taxisDataFlow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "taxisInput",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "ModifyColumns1",
					"description": "Autogenerated by data preview actions"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          pickup as string,",
				"          dropoff as string,",
				"          passengers as short,",
				"          distance as double,",
				"          fare as double,",
				"          tip as double,",
				"          tolls as double,",
				"          color as string,",
				"          payment as string,",
				"          pickup_zone as string,",
				"          dropoff_zone as string,",
				"          pickup_borough as string,",
				"          dropoff_borough as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     partitionBy('hash', 1)) ~> source1",
				"source1 derive(dropoff = trim(dropoff),",
				"          pickup = trim(pickup)) ~> ModifyColumns1",
				"ModifyColumns1 derive(total = (fare+tip+tolls),",
				"          totalByDistance = (fare+tip+tolls)/distance) ~> derivedColumn1"
			]
		}
	}
}